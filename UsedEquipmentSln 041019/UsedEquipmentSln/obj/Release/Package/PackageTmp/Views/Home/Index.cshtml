<div id="wnd_task" style="padding:20px" hidden>
    <div class="card">
        <div class="form-group" style="font-size: 12px">
            <div class="col-md-12">
                <br />
                <div class="col-md-4">Job ID :</div>
                <div class="col-md-6"><input type="text" id="txt_jobid" class="k-textbox" style="width: 100%" /></div>
            </div>
            <div class="col-md-12">
                <br />
                <div class="col-md-4">DB Server Name :</div>
                <div class="col-md-6"><input type="text" id="txt_svname" class="k-textbox" style="width: 100%" /></div>
            </div>
            <div class="col-md-12">
                <br />
                <div class="col-md-4">Database Name :</div>
                <div class="col-md-6"><input type="text" id="txt_dbname" class="k-textbox" style="width: 100%" /></div>
            </div>
            <div class="col-md-12">
                <br />
                <div class="col-md-4">Script Location :</div>
                <div class="col-md-6"><input type="text" id="txt_script_location" class="k-textbox" style="width: 100%" /></div>
            </div>
            <div class="col-md-12">
                <br />
                <div class="col-md-4">Interval Type (R/H/M) :</div>
                <div class="col-md-6"><input type="text" id="txt_type" maxlength="1" class="k-textbox" style="width: 100%" /></div>
            </div>
            <div class="col-md-12">
                <br />
                <div class="col-md-4">Duration :</div>
                <div class="col-md-6"><input type="number" min="0" id="txt_duration" class="k-textbox" style="width: 100%" /></div>
            </div>
            <div class="col-md-12">
                <br />
                <div class="col-md-4">Active Status :</div>
                <div class="col-md-4">
                    <input type="checkbox" id="chk_task" />
                </div>
            </div>
            
            <div class="clearfix"></div>
            <br /><br />
            <div class="col-md-11">
                <center>
                    <button type="button" id="btnSaveTask" class="btn btn-round btn-success" style="width: 100%" onclick="saveTask()">SAVE</button>
                </center>
            </div>

        </div>
    </div>
</div>

