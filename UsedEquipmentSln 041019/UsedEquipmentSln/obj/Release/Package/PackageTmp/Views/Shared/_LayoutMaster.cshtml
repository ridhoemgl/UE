<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    @*<meta name="viewport" content="width=device-width, initial-scale=1">*@
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>Integrated System Used Equipment - PT. Pamapersada Nusantara</title>

    <!-- Global stylesheets -->
    <link href="@ViewBag.pathParent/assets/css/icons/icomoon/styles.css" rel="stylesheet" type="text/css">
    <link href="@ViewBag.pathParent/assets/css/bootstrap.css" rel="stylesheet" type="text/css">
    <link href="@ViewBag.pathParent/assets/css/core.css" rel="stylesheet" type="text/css">
    <link href="@ViewBag.pathParent/assets/css/components.css" rel="stylesheet" type="text/css">
    <link href="@ViewBag.pathParent/assets/css/colors.css" rel="stylesheet" type="text/css">
    <link href="@ViewBag.pathParent/assets/css/sweetalert.css" rel="stylesheet" type="text/css" />
    <link href="@ViewBag.pathParent/assets/jconfirm/jquery-confirm.min.css" rel="stylesheet" />
    <link rel="shortcut icon" type="image/x-icon" href="@ViewBag.pathParent/assets/images/icon_isue.ico" />
    <!-- /global stylesheets -->
    <!-- Core JS files -->
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/core/libraries/jquery.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/Kendo/js/jquery.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/jquery-3.2.1.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/Scripts/jquery-1.10.2.min.js"></script>
   
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/core/libraries/bootstrap.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/loaders/pace.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/loaders/blockui.min.js"></script>
    <!-- /core JS files -->
    <!-- Theme JS files -->
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/visualization/d3/d3.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/visualization/d3/d3_tooltip.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/forms/styling/uniform.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/forms/styling/switchery.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/forms/styling/switch.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/forms/selects/bootstrap_multiselect.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/ui/moment/moment.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/pickers/daterangepicker.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/ui/nicescroll.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/pagination/bootpag.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/pagination/bs_pagination.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/pagination/datepaginator.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/pagination/datepaginator.min.js"></script>
    
    <script src="~/assets/jconfirm/jquery-confirm.min.js"></script>
    @*<script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/visualization/echarts/echarts-bar.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/plugins/visualization/echarts/echarts.js"></script>*@

    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/core/app.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/pages/layout_fixed_custom.js"></script>
    <!-- /theme JS files -->

    <!-- Kendo JS files -->
    <link href="@ViewBag.pathParent/Kendo/css/kendo.uniform.min.css" rel="stylesheet" />
    <link href="@ViewBag.pathParent/Kendo/css/kendo.common.min.css" rel="stylesheet" />
    <script type="text/javascript" src="@ViewBag.pathParent/Kendo/jszip.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/assets/js/sweetalert.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/Kendo/js/kendo.all.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/Kendo/js/kendo.aspnetmvc.min.js"></script>
    <!-- /Kendo JS files -->

    <style>
        .k-grid {
            font-size: 12px;
        }

            .k-grid td {
                line-height: 2em;
            }
    </style>

</head>
<body>
    <!-- Main navbar -->
    <div class="navbar navbar-inverse">
        <div class="navbar-header">
            <a class="navbar-brand" href="#" style=" width: 200px; height: 100%;">
                <img src="@ViewBag.pathParent/assets/images/ISUE.jpg" style="width: 140%;height: 200%;" />
            </a>
            <ul class="nav navbar-nav visible-xs-block">
                <li><a data-toggle="collapse" data-target="#navbar-mobile"><i class="icon-tree5"></i></a></li>
                <li><a class="sidebar-mobile-main-toggle"><i class="icon-paragraph-justify3"></i></a></li>
            </ul>
        </div>
        <div class="navbar-collapse collapse" id="navbar-mobile">
            <ul class="nav navbar-nav">
                <li><a class="sidebar-control sidebar-main-toggle hidden-xs"><i class="icon-paragraph-justify3"></i></a></li>
            </ul>
            <p class="navbar-text"><span class="label bg-success">Online</span></p>
            <ul class="nav navbar-nav navbar-right">
                <li class="dropdown dropdown-user">
                    <a class="dropdown-toggle" data-toggle="dropdown">
                        <img src="@ViewBag.pathParent/ContectLayout/img/user.png" alt="" class="profile-pic">
                        @*<span>@Session["Name"]</span>*@
                        <i class="caret"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-menu-right">
                        <li>
                            <div class="dw-user-box">
                                <center>
                                    <div class="u-img"><img src="@ViewBag.pathParent/ContectLayout/img/user.png" alt="user"></div>
                                    <div class="u-text">
                                        <h4>@Session["Name"]</h4>
                                        <p class="text-muted">@Session["description"] - @Session["distrik"]</p>
                                    </div>
                                </center>
                            </div>
                        </li>
                        <li role="separator" class="divider"></li>
                        <li><a href="@ViewBag.pathParent/Login/Profiles"><i class="icon-user"></i> Change Profile</a></li>
                        <li id="change_password"><a href="@ViewBag.pathParent/Login/ChangePassword"><i class="glyphicon glyphicon-lock"></i> Change Password</a></li>
                        <li><a href="@ViewBag.pathParent/Login/logout"><i class="icon-switch2"></i> Logout</a></li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
    <!-- /main navbar -->
    <!-- Page container -->
    <div class="page-container">
        <!-- Page content -->
        <div class="page-content">
            <!-- Main sidebar -->
            <div class="sidebar sidebar-main">
                <div class="sidebar-content">
                    <!-- User menu -->
                    <div class="sidebar-user">
                        <div class="category-content">
                            <div class="media">
                                <a href="#" class="media-left"><img src="@ViewBag.pathParent/ContectLayout/img/user.png" class="img-circle img-sm" alt=""></a>
                                <div class="media-body">
                                    <span class="media-heading text-semibold">@Session["Name"]</span>
                                    <div class="text-size-mini text-muted">
                                        @Session["description"] - @Session["distrik"]
                                    </div>
                                </div>
                                <div class="media-right media-middle">
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /user menu -->
                    <!-- Main navigation -->
                    <div class="sidebar-category sidebar-category-visible">
                        <div class="category-content no-padding">
                            <ul class="navigation navigation-main navigation-accordion">
                                <!-- Main -->
                                @Html.Raw(ViewBag.leftMenu)
                            </ul>
                        </div>
                    </div>
                    <!-- /main navigation -->
                </div>
            </div>
            <!-- /main sidebar -->
            <!-- Main content -->
            <div class="content-wrapper">
                <!-- Page header -->
                <div class="page-header page-header-default">
                    <div class="page-header-content">
                        <div class="page-title">
                            <h4><span class="text-semibold">@ViewBag.dashboard</span></h4>
                        </div>
                    </div>
                    <div class="breadcrumb-line">
                        <ul class="breadcrumb">
                            <li><i class="icon-home2 position-left"></i> <a href="Home">Home</a></li>
                            <li class="active">@ViewBag.dashboardSmall</li>
                        </ul>
                    </div>
                </div>
                <!-- /page header -->
                <!-- Content area -->
                <div class="content">
                    @RenderBody()
                    <!-- Footer -->
                    <div class="footer text-muted">
                        &copy; 2019. <a href="#">Integrated System Used Equipment</a> by <a href="#" target="_blank">Pamapersada Nusantara</a>
                    </div>
                    <!-- /footer -->
                </div>
                <!-- /content area -->
            </div>
            <!-- /main content -->
        </div>
        <!-- /page content -->
    </div>
    <!-- /page container -->
</body>
</html>

<script>

    $(document).ready(function () {
        
    })

</script>
